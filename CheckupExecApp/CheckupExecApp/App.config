<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
  </configSections>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" autoReload="true">
    <targets>
      <target name="console" type="Console" layout="${longdate}|${logger}|${level}|${message}" />
      <target name="file" xsi:type="File" layout="${longdate} ${logger} ${message}" filename="${basedir}/CheckupExecLog.txt" archiveFileName="${basedir}/archives/log.{#####}.txt" archiveAboveSize="5000000" archiveNumbering="Sequence" concurrentWrites="true" keepFileOpen="false" />
    </targets>
    <rules>
      <logger name="*" minlevel="Debug" appendTo="console, file" />
    </rules>
   </nlog>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
</configuration>